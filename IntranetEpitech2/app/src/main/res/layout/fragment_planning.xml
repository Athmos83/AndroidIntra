<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.epitech.intra.intranetepitech.PlanningActivity$PlaceholderFragment"
    android:background="#ff2fb5ff">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/buttonprev"
        android:onClick="ButtonClickPrev()" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/buttonnext"
        android:onClick="ButtonClickNext()"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/planning"
        android:layout_alignEnd="@+id/planning" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id ="@+id/planning"
        android:layout_below="@+id/buttonprev">
    </ListView>
</RelativeLayout>
